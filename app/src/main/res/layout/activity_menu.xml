<?xml version="1.0" encoding="utf-8"?>
<com.sothree.slidinguppanel.SlidingUpPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    sothree:umanoDragView="@+id/relativeLayout_details_header"
    sothree:umanoOverlay="true"
    sothree:umanoPanelHeight="90dp"
    sothree:umanoParallaxOffset="90dp"
    sothree:umanoShadowHeight="5dp">

    <fragment
        android:id="@+id/fragment_menu"
        android:name="com.brookers.serveraid.menu.MenuFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white" />

    <fragment
        android:id="@+id/fragment_orderDetails"
        android:name="com.brookers.serveraid.orderdetails.OrderDetailsFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</com.sothree.slidinguppanel.SlidingUpPanelLayout>